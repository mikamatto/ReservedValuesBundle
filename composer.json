{
    "name": "mikamatto/reserved-values-bundle",
    "description": "A Symfony bundle to restrict reserved values in form fields such as usernames or slugs, with support for exact matches and regular expressions.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikamatto",
            "email": "mick@mikamatto.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mikamatto\\ReservedValuesBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}